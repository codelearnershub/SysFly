@using AirlineMS.Models.Dtos
@model BranchDto
@{
    ViewData["Title"] = "Update Branch";
}
<div>
    <div class="text-center">
        <h3>@ViewData["Title"]</h3>
    </div>
    <div class="row">
        <div class="col-lg-3"></div>
        <div class="col-lg-6">
            <form asp-controller="Branch" asp-action="Update" asp-route-agencyId="">
                <div>
                    <label for="">Name</label>
                    <input type="" name="Name" value="@Model.Name"/>
                </div>
                <div>
                    <label for="">PhoneNumber</label>
                    <input type="" name="PhoneNumber" value="@Model.PhoneNumber"/>
                </div>
                <div>
                    <label for="">Email</label>
                    <input type="" name="Email" value="@Model.Email"/>
                </div>
                <div>
                    <label for="">Address</label>
                    <input type="" name="Address" value="@Model.Address"/>
                </div>
                <div>
                    <button class="btn btn-primary">Update Branch</button>
                </div>
            </form>
        </div>
        <div class="col-lg-3"></div>
    </div>
</div>